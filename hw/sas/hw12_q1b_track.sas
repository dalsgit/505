options ls=78;
title 'Cluster Analysis - Track';
data wood;
  infile 'x:\505\HW12_trackw.dat';
  input country $ d100 d200 d400 d800 d1500 d3000 marathon;
  run;
proc sort;
  by country;
proc cluster method=single outtree=clust1;
  var d100 d200 d400 d800 d1500 d3000 marathon;
  id country;
  run;
proc tree horizontal nclusters=6 out=clust2;
  id country;
  run;
proc sort;
  by country;
  run;
proc print;
  run;
data combine;
  merge wood clust2;
  by country;
  run;
proc glm;
  class cluster;
  model d100 d200 d400 d800 d1500 d3000 marathon = cluster;
  means cluster;
  run;
quit;