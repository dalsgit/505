options ls=78;
title 'Cluster Analysis K - Track';
data wood;
  infile 'x:\505\HW12_trackw.dat';
  input country $ d100 d200 d400 d800 d1500 d3000 marathon;
  run;
proc sort;
  by country;
proc fastclus maxclusters=4 replace=random out=clust2;
  var d100 d200 d400 d800 d1500 d3000 marathon;
  id country;
  run;
data combine;
  merge wood clust2;
  by country;
  run;
proc print;
  run;  
quit;